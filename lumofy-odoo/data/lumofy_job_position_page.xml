<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_job_position_form_inherit" model="ir.ui.view">
        <field name="name">lumofy.jobs.inherit.lumofy.jobs</field>
        <field name="model">hr.job</field>
        <field name="inherit_id" ref="hr.view_hr_job_form" />
        <field name="priority" eval="120" />
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Lumofy Data">
                    <group>
                        <field name="lumofy_job_level" />
                        <field name="lumofy_job_function_id" />
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>